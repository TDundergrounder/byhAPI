package com.beyondh.model.hotel;

import com.beyondh.model.order.RoomPrice;

public class HourRentMemberPriceQueryResultInfo {
    private String CheckinType;
    private RoomPrice[] Prices;

    public HourRentMemberPriceQueryResultInfo(String checkinType, RoomPrice[] prices) {
        CheckinType = checkinType;
        Prices = prices;
    }

    public String getCheckinType() {
        return CheckinType;
    }

    public void setCheckinType(String checkinType) {
        CheckinType = checkinType;
    }

    public RoomPrice[] getPrices() {
        return Prices;
    }

    public void setPrices(RoomPrice[] prices) {
        Prices = prices;
    }
}
