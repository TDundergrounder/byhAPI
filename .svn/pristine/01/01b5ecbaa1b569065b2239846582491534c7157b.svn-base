package com.beyondh.model;

import java.io.Serializable;

public class OrderByRequest implements Serializable {
    private String OrderBy;
    public String getOrderBy()
    {
        return OrderBy;
    }
    public void setOrderBy(String value)
    {
        OrderBy = value;
    }

    private boolean Asc;
    public boolean getAsc()
    {
        return Asc;
    }
    public void setAsc(boolean value)
    {
        Asc = value;
    }

    public OrderByRequest(String orderBy, boolean asc) {
        OrderBy = orderBy;
        Asc = asc;
    }
}
